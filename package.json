{
  "name": "teensy-extension",
  "displayName": "teensy",
  "description": "Make your urls teensy with this free extension!",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "teensy.teensyLink",
        "title": "Teensy A Link"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "18.11.19",
    "@types/node-fetch": "2",
    "@types/vscode": "^1.75.0",
    "@vscode/test-electron": "^2.2.3",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "node-fetch": "2"
  }
}

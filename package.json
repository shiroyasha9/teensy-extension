{
  "name": "teeny-extension",
  "displayName": "teeny",
  "description": "Make your urls teeny with this free extension!",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:teeny.teenyLink"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "teeny.teenyLink",
        "title": "Teeny A Link"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/node-fetch": "2",
    "@types/vscode": "^1.74.0",
    "@vscode/test-electron": "^2.2.1",
    "glob": "^8.0.3",
    "mocha": "^10.2.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "node-fetch": "2"
  }
}

{
  "name": "teensy-extension",
  "displayName": "teensy",
  "description": "Make your long URLs short (teensies) with this extension straight away from VS Code. Fully Open Source.",
  "version": "1.0.0",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://teensy.tech/gh"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "teensy.teensyLink",
        "title": "Teensy A Link"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "18.11.19",
    "@types/node-fetch": "2",
    "@types/vscode": "^1.75.0",
    "@vscode/test-electron": "^2.2.3",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "node-fetch": "2"
  }
}
